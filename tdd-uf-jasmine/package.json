{
  "author": "Manulife University Engineering",
  "name": "tdd-jasmine",
  "version": "1.0.0",
  "description": "TDD using jasmine",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "babel-node run.js",
    "test:clean": "babel-node node_modules/jasmine/bin/jasmine",
    "test:report": "jest --config=jest.config.json -i --detectOpenHandles"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/node": "latest",
    "@babel/preset-env": "latest",
    "eslint": "latest",
    "eslint-config-airbnb-base": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-prettier": "latest",
    "jest-cli": "latest",
    "jest-html-reporter": "latest",
    "sinon": "latest"
  },
  "dependencies": {
    "@babel/preset-react": "^7.7.0",
    "@types/jasmine": "latest",
    "@types/jest": "latest",
    "@types/mocha": "latest",
    "jasmine": "latest",
    "jasmine-console-reporter": "latest",
    "jasmine-node": "latest"
  }
}
