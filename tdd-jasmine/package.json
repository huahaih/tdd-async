{
  "author": "Manulife University Engineering",
  "name": "tdd-jasmine",
  "version": "1.0.0",
  "description": "TDD using jasmine",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test1": "babel-node node_modules/jasmine/bin/jasmine",
    "test": "babel-node run.js",
    "testing": "jest --config=jest.config.json -i --detectOpenHandles"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest-cli": "^24.3.1",
    "jest-html-reporter": "^2.5.0"
  },
  "dependencies": {
    "@types/jasmine": "^3.3.9",
    "@types/jest": "^24.0.11",
    "@types/mocha": "^5.2.6",
    "jasmine": "^3.3.1",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-node": "^2.0.1",
    "sinon": "^7.2.7"
  }
}