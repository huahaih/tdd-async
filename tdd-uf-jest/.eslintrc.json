{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "no-plusplus": "off",
    "no-restricted-syntax": "off",
    "no-mixed-operators": "off",
    "no-use-before-define": "off",
    "prefer-const": "off",
    "no-undef": "off",
    "no-else-return": "off",
    "no-param-reassign": "off",
    "no-bitwise": "off",
    "no-restricted-globals": "off",
    "no-console": "off"
  }
}